{
  "name": "foodspend",
  "private": true,
  "version": "0.4.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/gen-icons.mjs && tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:all": "npm run build && firebase deploy --only hosting,firestore",
    "firestore:reset:users": "firebase firestore:delete -r --force users --project spendtrack-x",
    "firestore:reset:all": "firebase firestore:delete --all-collections --force --project spendtrack-x"
  },
  "dependencies": {
    "firebase": "10.14.1",
    "idb": "^7.1.1",
    "lucide-react": "0.378.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "canvas": "^3.2.0",
    "firebase-tools": "^13.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0"
  }
}
